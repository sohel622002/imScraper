{
  "name": "imScraper",
  "version": "1",
  "manifest_version": 3,
  "minimum_chrome_version": "10.0",
  "devtools_page": "app/src/index.html",
  "icons": {
    "16": "app/assets/16.png",
    "32": "app/assets/32.png",
    "48": "app/assets/48.png",
    "128": "app/assets/spider.png"
  },
  "content_scripts": [
    {
      "matches":[
        "<all_urls>"
      ],
      "js": [
        "app/script/script.js"
      ]
    }
  ]
}
